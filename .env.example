# Telegram App ID (从 https://my.telegram.org 获取)
APP_ID=

# Telegram App API Hash (从 https://my.telegram.org 获取)
APP_API_HASH=

# Telegram String Session (通过本地脚本生成)
STRING_SESSION=

# 通知 Webhook URL (你想将消息转发到的地址)
DEPLOYMENT_URL=

# Cron 认证 Token (保护你的 cron 端点)
CRON_AUTH_TOKEN=

# 监控群组ID列表，多个ID用逗号分隔,注意ID前面的负号也要保留(通常以-100开头)
MONITOR_CHAT_IDS=

# 不需要监控的群组ID,注意ID前面的负号也要保留(通常以-100开头)
NOT_MONITOR_CHAT_IDS=

# 通知发送的目标群组ID,注意ID前面的负号也要保留(通常以-100开头)
NOTIFICATION_CHAT_ID=

# Telegram Bot Token (用于发送通知的机器人)
TELEGRAM_BOT_TOKEN=

# 请将下面的ID替换为实际的抽奖机器人ID,优先监控的用户ID（多个ID用逗号分隔）
TARGET_USER_IDS=

# 用户特定关键词（多个关键词用逗号分隔）
USER_KEYWORDS=

# 通用监控关键词,多个关键词用逗号分隔(默认为空,为空则不监控)
MONITOR_KEYWORDS=

# 自动删除消息的时间阈值（分钟），默认10分钟，测试时可以设置更小的值
AUTO_DELETE_MINUTES=5

# 删除历史消息模式，设置为true时将删除监控群组中的历史消息
DELETE_HISTORY_MODE=true

# 删除消息数量限制，0表示删除所有消息
DELETE_MESSAGES_LIMIT=0