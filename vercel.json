{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.5.2"
    },
    "functions/start-monitor.func/index.js": {
      "runtime": "vercel-edge@1.0.0"
    }
  },
  "routes": [
    {
      "src": "/setWebHook",
      "dest": "/api/setWebHook.php"
    },
    {
      "src": "/webhook",
      "dest": "/api/webhook.php"
    },
    {
      "src": "/startMonitor",
      "dest": "/functions/start-monitor.func/index.js"
    }
  ]
}